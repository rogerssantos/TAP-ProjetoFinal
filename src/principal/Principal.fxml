<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="apRoot" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="principal.PrincipalController">
   <children>
      <VBox>
         <children>
            <Button mnemonicParsing="false" onAction="#loadTelaJogo" text="Jogar" />
            <Button mnemonicParsing="false" onAction="#loadTelaProfessor" text="Professores" />
            <Button mnemonicParsing="false" text="Criar Jogos ou Editar" />
            <Button mnemonicParsing="false" onAction="#loadTelaConfiguracao" text="Configurações" />
            <Button mnemonicParsing="false" onAction="#botaoSair" text="Sair" />
         </children>
      </VBox>
   </children>
</AnchorPane>
